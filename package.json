{
  "name": "worriedly",
  "version": "0.1.0",
  "author": "Wang Guan <momocraft@gmail.com>",
  "private": false,
  "index": "build/lib/index",
  "bin": {
    "worriedly": "bin/worriedly"
  },
  "files": [
    "bin",
    "build",
    "!build/__test__/"
  ],
  "scripts": {
    "start": "node build/bin/worriedly",
    "start:ts": "ts-node src/bin/worriedly",
    "build": "rm -rf dist && tsc --project tsconfig.cjs.json",
    "build:demo": "rm -rf docs/latest public && gatsby build",
    "dev": "gatsby develop",
    "local:install": "yarn build && yarn global add $PWD",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit",
    "lint": "    eslint 'src/**/*.@(ts|js|tsx|jsx)' 'bin/*' 'demo/*.tsx'",
    "lint:fix": "eslint 'src/**/*.@(ts|js|tsx|jsx)' 'bin/*' 'demo/*.tsx' --fix"
  },
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/jokester/worriedly/issues"
  },
  "homepage": "https://github.com/jokester/worriedly#readme",
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@jokester/ts-commonutil": "^0.2.2",
    "@zxing/library": "^0.17.1",
    "argparse": "<2",
    "chalk": "^4.1.0",
    "classnames": "^2.2.6",
    "emotion-theming": "^10.0.27",
    "fp-ts": "^2.8.3",
    "js-sha1": "^0.6.0",
    "qrcode-generator": "^1.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tslib": "^2.0.1",
    "use-constant": "^1.0.0",
    "webrtc-adapter": "^7.7.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/argparse": "<2",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26",
    "@types/node": "^12",
    "@types/react": "^16.9.49",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "7.9.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "gatsby": "^2.24.65",
    "gatsby-plugin-react-helmet": "^3.3.11",
    "gatsby-plugin-sass": "^2.3.13",
    "gatsby-plugin-typescript": "^2.4.20",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.12",
    "jest": "^26",
    "prettier": "^2.1.2",
    "react-helmet": "^6.1.0",
    "sass": "^1.26.11",
    "tailwindcss": "^1.8.10",
    "ts-jest": "^26",
    "ts-loader": "8.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "resolutions": {
    "@types/node": "12"
  }
}
