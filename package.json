{
  "name": "worriedly",
  "version": "0.1.0",
  "author": "Wang Guan <momocraft@gmail.com>",
  "private": false,
  "index": "build/lib/index",
  "bin": {
    "worriedly": "bin/worriedly"
  },
  "files": [
    "bin",
    "build",
    "!build/__test__/"
  ],
  "scripts": {
    "start": "node build/bin/worriedly",
    "start:ts": "ts-node src/bin/worriedly",
    "build": "rm -rvf build && tsc",
    "local:install": "yarn build && yarn global add $PWD",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit",
    "lint": "    eslint '**/*.@(ts|js|tsx|jsx)' 'bin/*'",
    "lint:fix": "eslint '**/*.@(ts|js|tsx|jsx)' 'bin/*' --fix"
  },
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/jokester/worriedly/issues"
  },
  "homepage": "https://github.com/jokester/worriedly#readme",
  "dependencies": {
    "argparse": "^1.0.10",
    "chalk": "^2.4.2",
    "preact": "^10.0.0",
    "qrcode-generator": "^1.4.4",
    "tslib": "^1.10.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/argparse": "^1.0.36",
    "@types/jest": "^24",
    "@types/node": "<11",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24",
    "prettier": "^1.18.2",
    "ts-jest": "^24",
    "ts-node": "^8.1.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.0"
  }
}
