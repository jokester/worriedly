{
  "name": "worriedly",
  "version": "0.1.0",
  "author": "Wang Guan <momocraft@gmail.com>",
  "private": false,
  "index": "build/lib/index",
  "bin": {
    "worriedly": "bin/worriedly"
  },
  "files": [
    "bin",
    "build",
    "!build/__test__/"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "start": "NODE_ENV=production next start",
    "relay-codegen": "yarn run relay-compiler --schema ../api-spec/schema.graphql --src ./src/ --watchman false --language=typescript $@",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit",
    "lint": "    eslint '**/*.@(ts|js|tsx|jsx)'",
    "lint:fix": "eslint --fix '**/*.@(ts|js|tsx|jsx)'"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jokester/worriedly/issues"
  },
  "homepage": "https://github.com/jokester/worriedly#readme",
  "dependencies": {
    "@chakra-ui/react": "^1.6.12",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@jokester/ts-commonutil": "^0.3.4",
    "@next/bundle-analyzer": "<12",
    "@zxing/library": "^0.18.6",
    "argparse": "<2",
    "chalk": "^4.1.0",
    "classnames": "^2.2.6",
    "emotion-theming": "^10.0.27",
    "fp-ts": "<3",
    "framer-motion": "<5",
    "js-sha1": "^0.6.0",
    "next": "<12",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.4.0",
    "qrcode-generator": "^1.4.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-use": "^17.3.1",
    "tslib": "^2",
    "use-constant": "^1.1.0",
    "webrtc-adapter": "^8.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15",
    "@types/argparse": "<2",
    "@types/jest": "^27",
    "@types/node": "12",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gts": "^3.1.0",
    "jest": "^27",
    "next-transpile-modules": "^9.0.0",
    "sass": "^1.43.4",
    "ts-jest": "^27",
    "typescript": "^4.4"
  },
  "resolutions": {
    "@types/node": "12",
    "tslib": "^2"
  },
  "engines": {
    "node": "^12 || ^14"
  }
}
