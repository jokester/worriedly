{
  "name": "worriedly",
  "version": "0.1.0",
  "author": "Wang Guan <momocraft@gmail.com>",
  "private": false,
  "index": "build/lib/index",
  "bin": {
    "worriedly": "bin/worriedly"
  },
  "files": [
    "bin",
    "build",
    "!build/__test__/"
  ],
  "scripts": {
    "start": "node build/bin/worriedly",
    "start:ts": "ts-node src/bin/worriedly",
    "build": "rm -rf dist && tsc --project tsconfig.cjs.json",
    "build:demo": "rm -rf docs/latest public && gatsby build && mv public docs/latest",
    "dev": "gatsby develop",
    "local:install": "yarn build && yarn global add $PWD",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit",
    "lint": "    eslint 'src/**/*.@(ts|js|tsx|jsx)' 'bin/*' 'demo/*.tsx'",
    "lint:fix": "eslint 'src/**/*.@(ts|js|tsx|jsx)' 'bin/*' 'demo/*.tsx' --fix"
  },
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/jokester/worriedly/issues"
  },
  "homepage": "https://github.com/jokester/worriedly#readme",
  "dependencies": {
    "argparse": "^1.0.10",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "js-sha1": "^0.6.0",
    "qrcode-generator": "^1.4.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "tslib": "^1.10.0",
    "webrtc-adapter": "^7.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/argparse": "^1.0.36",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24",
    "@types/node": "<11",
    "@types/react": "^16.9.13",
    "@types/react-helmet": "^5.0.14",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "gatsby": "^2.18.4",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sass": "^2.1.24",
    "gatsby-plugin-typescript": "^2.1.20",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.8",
    "jest": "^24",
    "prettier": "^1.18.2",
    "react-helmet": "^5.2.1",
    "sass": "^1.23.7",
    "ts-jest": "^24",
    "ts-loader": "6.2.1",
    "ts-node": "^8.1.0",
    "typescript": "^3.7.2"
  },
  "resolutions": {
    "eslint": "6.5",
    "@types/node": "10"
  }
}
